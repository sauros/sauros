[var primes]
[var is_prime [lambda [n] [
   [block
      [if [or [== n 2] [== n 3]] [push primes n] [
         [if [and [ > n 1] [and [% n 2] [% n 3]] ] [
            [block
               [var i 5]
               [var pass 1]
               [loop [and pass [<= [* i i] n]] [set i [+ 6 i]] [
                  [block
                     [if [not [% n i]] [set pass 0]]
                     [if [not [% n [+ i 2]]] [set pass 0]]
                  ]
               ]]
               [if [== 1 pass] [push primes n]]
            ]
         ]]
      ]]
   ]
]]]

[var count 0]
[loop [< count 100] [set count [+ 1 count]] [
   [is_prime count]
]]
[put primes]

