[use "math"]

[assert "math_package.saur > pi" [== math.pi 3.14159265359]]
[assert "math_package.saur > phi" [== math.phi 1.61803398875]]
[assert "math_package.saur > e" [== math.e 2.71828182845]]

[assert "math_package.saur > log" [== [math.log 4] 1.386294]]
[assert "math_package.saur > log2" [== [math.log2 10] 3.321928]]
[assert "math_package.saur > log10" [== [math.log10 8] 0.903090]]

[assert "math_package.saur > sin" [== [math.sin 1] 0.841471]]
[assert "math_package.saur > cos" [== [math.cos 1] 0.540302]]
[assert "math_package.saur > tan" [== [math.tan 1] 1.557408]]

[assert "math_package.saur > asin" [== [math.asin 1] 1.570796]]
[assert "math_package.saur > acos" [== [math.acos 1] 0.000000]]
[assert "math_package.saur > atan" [== [math.atan 1] 0.785398]]

[assert "math_package.saur > asinh" [== [math.asinh 1] 0.881374]]
[assert "math_package.saur > acosh" [== [math.acosh 1] 0.000000]]
[assert "math_package.saur > atanh" [== [math.atanh 0.5] 0.549306]]

[assert "math_package.saur > exp" [== [math.exp 8] 2980.957987]]
[assert "math_package.saur > sqrt" [== [math.sqrt 18] 4.242641]]

[assert "math_package.saur > abs" [== [math.abs -2.5] 2.5]]
[assert "math_package.saur > ceil" [== [math.ceil 1.3] 2.0]]
[assert "math_package.saur > floor" [== [math.floor 1.9] 1.0]]

[assert "math_package.saur > pow" [== [math.pow 2 8] 256.000000]]