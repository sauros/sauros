; Pkg name
[var pkg_name "random"]

[var authors "Bosley"]
[var license "MIT"]
[var version "0.0.0"]

; C++ shared lib to link to
[var library_file "random.lib"]
[var library_functions [list
   "_pkg_random_string_"
   "_pkg_random_alpha_string_"
   "_pkg_random_sourced_string_"
   "_pkg_random_uniform_int_"
   "_pkg_random_uniform_real_"
]]

; Files to include under the current pkg
[var source_files [list
   "random.saur"
]]