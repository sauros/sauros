{
   "name": "vsc-sauros-lang-extension",
   "displayName": "Sauros Syntax Highlighting",
   "description": "A VSC extension to support Sauros syntax",
   "publisher": "Josh A. Bosley",
   "version": "0.0.1",
   "engines": {
       "vscode": "^1.35.0"
   },
   "categories": [
       "Programming Languages"
   ],
   "contributes": {
       "languages": [{
           "id": "saur",
           "aliases": ["saur", "saur"],
           "extensions": [".saur"],
           "configuration": "./language-configuration.json"
       }],
       "grammars": [{
           "language": "saur",
           "scopeName": "source.saur",
           "path": "./syntaxes/saur.tmLanguage.json"
       }]
   }
}